{
  "name": "native-timer",
  "version": "1.0.0",
  "description": "A simple timer web app in native JavaScript. ",
  "main": "src/app.js",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "npm run clean; echo \"Files cleaned up.\" && mkdir -p dist",
    "build": "babel ./src/app.js --out-file ./dist/bundle.js --source-maps inline",
    "postbuild": "cp -r ./src/assets dist && cp ./src/index.html ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/805js/native-timer.git"
  },
  "keywords": [
    "Timer",
    "JavaScript",
    "Native",
    "Demo"
  ],
